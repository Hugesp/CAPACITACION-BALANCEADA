@model Entities.Models.Clientes


<div class="row">
    <div class="col s12">
        <form asp-action="_AddEdit" class="col s12" id="form">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div style="display:none">
                <input asp-for="IdCliente" class="form-control" />
            </div>
            <div class="row">
                <div class="input-field">
                    <label asp-for="Nombre"></label>
                    <input asp-for="Nombre" />
                </div>
            </div>
            <div class="row">
                <div class="input-field">
                    <label asp-for="Apellidop">Apellido Paterno</label>
                    <input asp-for="Apellidop" />
                </div>
            </div>
            <div class="row">
                <div class="input-field">
                    <label asp-for="Apellidom">Apellido Materno</label>
                    <input asp-for="Apellidom" />
                </div>
            </div>
            <div class="row">
                <label>Fecha de Nacimiento</label>
                <div class="input-field">
                    <input asp-for="FechaNacimiento" type="text" class="datepicker">
                </div>
            </div>
            <div class="row">
                <div class="input-field">
                    <label asp-for="Rfc"></label>
                    <input asp-for="Rfc" />
                </div>
            </div>
        </form>
    </div>
</div>

